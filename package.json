{
  "name": "glintcms-starter-intesso",
  "version": "1.1.11",
  "main": "server.js",
  "browser": "browser.js",
  "scripts": {
    "prepublish": "npm run link-local",
    "version": "NODE_ENV=production && npm run assets && npm run build",
    "init": "npm install assets-bundler && npm run prepublish && npm install && npm run install-local",
    "install-local": "ls -d1 $(pwd)/node_modules/{page,widget,wrap}-*/ | xargs -L 1 bash -c 'cd \"$0\" && pwd && npm install'",
    "install-linked": "find $(pwd)/node_modules -type l -maxdepth 1 | xargs -L 1 bash -c 'cd \"$0\" && pwd && npm install'",
    "link-local": "assets-bundler --src './lib/*' --dest './node_modules/*' -rf",
    "link-glint": "assets-bundler --src '../glint-*' --dest 'node_modules/glint-*' -rf",
    "cleanup": "rm -rf node_modules/ && rm -rf lib/*/node_modules/",
    "assume-unchanged": "git update-index --assume-unchanged datastore/glint/role/7032a580-5c4e-11e5-b149-5bd68197214e.json; git update-index --assume-unchanged datastore/glint/user/7032a580-5c4e-11e5-b149-5bd68197214e.json",
    "assets": "rm -rf public/assets/ && assets-bundler --src node_modules/:module/public --dest public/assets/:module -rf",
    "build": "npm run assets; if [ \"$NODE_ENV\" == \"development\" ]; then browserify browser.js -o public/build/bundle.js --debug; else browserify -p bundle-collapser/plugin -t envify -g [ uglifyify -x '.js' --ignore '**/public/*' ] browser.js -o public/build/bundle.js; fi",
    "watch": "watchify -t [ envify purge ] -t [brfs] browser.js -o public/build/bundle.js --debug &",
    "www": "export NODE_ENV=development && npm run assets && npm run watch && nodemon -e js,ejs,dot start.js",
    "start": "if [ -z $NODE_ENV ]; then NODE_ENV=production; fi; echo NODE_ENV=$NODE_ENV && npm run build && node start"
  },
  "bundledDependencies": [
    "page-home",
    "page-adapter",
    "page-article",
    "page-articles",
    "page-auth",
    "page-auth-access",
    "page-contact",
    "page-error",
    "page-filemanager",
    "page-home",
    "page-i18n",
    "page-project",
    "page-projects",
    "page-translate",
    "page-upload",
    "widget-articles-featured",
    "widget-menu",
    "widget-projects-featured",
    "wrap-layout"
  ],
  "dependencies": {
    "assets-bundler": "^2.0.0",
    "clone": "^1.0.2",
    "compression": "^1.5.0",
    "connect-flash": "^0.1.1",
    "connect-is-bot": "^1.0.0",
    "cookie-parser": "~1.3.3",
    "debug": "^2.2.0",
    "defaults": "^1.0.2",
    "dot": "^1.0.3",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "glint-adapter": "^1.0.3",
    "glint-adapter-ajax": "^1.0.3",
    "glint-adapter-elasticsearch": "^1.0.3",
    "glint-adapter-fs": "^1.0.5",
    "glint-block": "^1.0.2",
    "glint-block-ckeditor": "^1.0.4",
    "glint-block-image": "^1.0.2",
    "glint-block-image-attribute": "^1.0.2",
    "glint-block-markdown": "^1.0.8",
    "glint-block-meta": "^1.0.2",
    "glint-block-text": "^1.0.3",
    "glint-container": "^1.0.2",
    "glint-i18n": "^1.0.4",
    "glint-plugin-adapter-dates": "^1.0.2",
    "glint-plugin-adapter-expires": "^1.0.2",
    "glint-plugin-adapter-id": "^1.0.2",
    "glint-plugin-block-style-editable": "^1.0.2",
    "glint-plugin-wrap-container-place": "^1.0.2",
    "glint-plugin-wrap-i18n": "^1.0.2",
    "glint-session": "^1.0.2",
    "glint-socket-io": "^1.0.2",
    "glint-tasks": "^1.1.1",
    "glint-trigger": "^1.0.2",
    "glint-trigger-keyboard": "^1.0.2",
    "glint-trigger-sidenav": "^1.0.2",
    "glint-util": "^1.0.2",
    "glint-widget": "^1.0.2",
    "glint-wrap": "^1.0.2",
    "is-browser": "^2.0.1",
    "page.js": "^4.13.3",
    "serve-favicon": "^2.2.0",
    "subarg": "^1.0.0",
    "utils-merge": "^1.0.0"
  },
  "devDependencies": {
    "brfs": "^1.3.0",
    "browserify": "^8.1.3",
    "bundle-collapser": "^1.2.1",
    "envify": "^3.4.0",
