{
  "name": "glintcms-starter-intesso",
  "version": "1.0.7",
  "main": "server.js",
  "browser": "browser.js",
  "browserify": {
    "transform": [
      [
        "brfs"
      ],
      [
        "envify",
        {
          "_": "purge"
        }
      ],
      [
        "uglifyify"
      ]
    ]
  },
  "scripts": {
    "prepublish": "export NODE_ENV=production && npm run link-local",
    "install-local": "ls -d1 $(pwd)/node_modules/{page,widget,wrap}-*/ | xargs -L 1 bash -c 'cd \"$0\" && pwd && npm install'",
    "install-linked": "find $(pwd)/node_modules -type l -maxdepth 1 | xargs -L 1 bash -c 'cd \"$0\" && pwd && npm install'",
    "link-local": "assets-bundler --src './lib/*' --dest './node_modules/*'",
    "link-glint": "export NODE_ENV=development && assets-bundler --src '../glint-*' --dest 'node_modules/glint-*'",
    "cleanup": "rm -rf node_modules/ && rm -rf lib/*/node_modules/",
    "assets": "rm -rf public/assets/ && assets-bundler --src node_modules/:module/public --dest public/assets/:module",
    "build": "export NODE_ENV=development && npm run assets && npm run link-local && browserify browser.js -o public/build/bundle.js --debug",
    "build-production": "export NODE_ENV=production && npm run assets && npm run link-local && browserify -t envify -g uglifyify browser.js -o public/build/bundle.js",
    "watch": "watchify -t [ envify purge ] -t [brfs] browser.js -o public/build/bundle.js --debug &",
    "minify": "cat public/build/bundle.js | uglifyjs -o public/build/bundle.min.js",
    "start": "export NODE_ENV=development && npm run build && node start",
    "start-production": "export NODE_ENV=production && npm run build-production && node start",
    "www": "export NODE_ENV=development; npm run assets && npm run watch && nodemon -e js,ejs,dot start.js"
  },
  "bundledDependencies": [
    "page-home",
    "page-adapter",
    "page-article",
    "page-articles",
    "page-auth",
    "page-auth-access",
    "page-contact",
    "page-error",
    "page-filemanager",
    "page-home",
    "page-i18n",
    "page-project",
    "page-projects",
    "page-translate",
    "page-upload",
    "widget-articles-featured",
    "widget-menu",
    "widget-projects-featured",
    "wrap-layout"
  ],
  "dependencies": {
    "clone": "^1.0.2",
    "compression": "^1.5.0",
    "connect-flash": "^0.1.1",
    "connect-is-bot": "^1.0.0",
    "cookie-parser": "~1.3.3",
    "debug": "^2.2.0",
    "defaults": "^1.0.2",
    "dot": "^1.0.3",
    "ejs": "^2.3.4",
    "express": "~4.11.1",
    "glint-adapter": "^1.0.3",
    "glint-adapter-ajax": "^1.0.3",
    "glint-adapter-elasticsearch": "^1.0.3",
    "glint-adapter-fs": "^1.0.5",
    "glint-block": "^1.0.2",
    "glint-block-ckeditor": "^1.0.4",
    "glint-block-image": "^1.0.2",
    "glint-block-image-attribute": "^1.0.2",
    "glint-block-markdown": "^1.0.2",
    "glint-block-meta": "^1.0.2",
    "glint-block-text": "^1.0.3",
    "glint-container": "^1.0.2",
    "glint-i18n": "^1.0.4",
    "glint-plugin-adapter-dates": "^1.0.2",
    "glint-plugin-adapter-expires": "^1.0.2",
    "glint-plugin-adapter-id": "^1.0.2",
    "glint-plugin-block-style-editable": "^1.0.2",
    "glint-plugin-wrap-container-place": "^1.0.2",
    "glint-plugin-wrap-i18n": "^1.0.2",
    "glint-session": "^1.0.2",
    "glint-socket-io": "^1.0.2",
    "glint-tasks": "^1.1.1",
    "glint-trigger": "^1.0.2",
    "glint-trigger-keyboard": "^1.0.2",
    "glint-trigger-sidenav": "^1.0.2",
    "glint-util": "^1.0.2",
    "glint-widget": "^1.0.2",
    "glint-wrap": "^1.0.2",
    "is-browser": "^2.0.1",
    "page": "git://github.com/intesso/page.js.git",
    "serve-favicon": "^2.2.0",
    "utils-merge": "^1.0.0"
  },
  "devDependencies": {
    "assets-bundler": "^1.0.3",
    "browserify": "^8.1.3",
    "brfs": "^1.3.0",
    "envify": "^3.4.0",
    "tape": "^3.5.0",
