<div data-id="intro"><%- intro %></div>

<section>
    <% projectsAll.forEach(function(project){ %>
    <a class="project-item" href="/projects/<%- project.path %>">
        <div class="row row-pad row-rounded-weird">
            <div class="col-1-4">
                <img class="img-circle" src="<%- project.teaser.src %>" alt="<%- project.teaser.alt %>" style="height: 225px; width: 225px"/>
            </div>
            <div class="col-3-4">
                <h2><%- project.title %></h2>
                <div><%- project.short %></div>
            </div>
        </div>
    </a>
    <% }); %>
</section>

<div data-id="outro"><%- outro %></div>
