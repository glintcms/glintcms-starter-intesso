<div>
    <% articles.forEach(function(article){ %>

    <div class="article">
        <div>
            <a class="tagline center" href="/articles/<%- article.path %>">
                <%- article.title %>
            </a>
        </div>
        <small class="tagline-short">  <%- article.short %></small>
    </div>

    <% }); %>
</div>

