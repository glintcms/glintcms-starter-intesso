<div data-id="intro"><%- intro %></div>

<section>
    <% articlesAll.forEach(function(article){ %>

    <div class="row-article">

        <a class="tagline center" href="/articles/<%- article.path %>"><%- article.title %></a>

        <big class="center"><%- new Date(article.updatedAt).toLocaleDateString() %></big>

        <span class="center"><%- article.short %></span>

    </div>
    <hr>
    <% }); %>
</section>

<div data-id="outro"><%- outro %></div>
