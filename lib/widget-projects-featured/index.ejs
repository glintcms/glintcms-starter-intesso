<section>
    <% projects.forEach(function(project){ %>

    <div class="project">
        <div>
            <a class="tagline center" href="/projects/<%- project.path %>">
                <%- project.title %>
            </a>
        </div>
        <small class="tagline-short">  <%- project.short %></small>
    </div>

    <% }); %>
</section>

